<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        {{#showStatusMessage detailMessages}} {{/showStatusMessage}}
    </div>
</div>

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <table id="members">
            <tr>
                <th>Full Name</th>
                <th>Position</th>
                <th>Project</th>
                <th>Image</th>
            </tr>
            {{#each members}}
            <tr>
                <td>{{this.fullname}}</td>
                <td>{{this.position}}</td>
                <td>{{this.project}}</td>
                <td><img src="images/image8.jpg" width="100" height="100" alt="none"/></td>
            </tr>
            {{/each}}
        </table>

    </div>
</div>

<br/>
<br/>
<h1>
    Insert Member
</h1>

<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <form action="groupteam" method="post">
            <div class="form-group">
                <lable for="">Full Name *</lable>
                <input type="text" class="form-control" name="fullname" placeholder="Enter your full name...">
            </div>
            <div class="form-group">
                <lable for="">Project</lable>
                <input type="text" class="form-control" name="project" placeholder="Enter your project...">
            </div>
            <div class="form-group">
                <lable for="">Position</lable>
                <input type="text" class="form-control" name="position" placeholder="Enter your position...">
            </div>
            <div class="form-group">
                <button type="reset" class="btn btn-default">Reset</button>
                <button type="submit" class="btn btn-primary">Send</button>
            </div>
        </form>
    </div>
</div>